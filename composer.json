{
    "name": "kasitaw/api-key",
    "description": "User defined api key(using custom laravel guard) to enable client communicate with server for external integration in general",
    "keywords": ["kasitaw", "api-key", "api-integration", "api"],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "^7.0"
    },
    "require-dev": {
        "laravel/framework": "^7.0",
        "phpunit/phpunit": "^8.0|^9.0",
        "orchestra/testbench": "^5.0",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "Kasitaw\\ApiKey\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kasitaw\\ApiKey\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kasitaw\\ApiKey\\ApiKeyServiceProvider"
            ]
        }
    },
    "scripts": {
        "format": [
            "vendor/bin/php-cs-fixer fix"
        ],
        "format-dry-run": [
            "vendor/bin/php-cs-fixer fix --dry-run --diff"
        ]
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Norlihazmey Ghazali",
            "email": "norlihazmey.ghazali@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
